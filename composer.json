{
    "name": "bima/laravel-publisher",
    "description": "Bima laravel publisher library",
    "keywords": ["laravel", "pandawa", "bima"],
    "license": "MIT",
    "type": "library",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "borobudur/dci": "~0.1@dev",
        "borobudur/exception": "~0.1@dev",
        "borobudur/parameter": "~0.1@dev",
        "guzzlehttp/guzzle": "~6.0",
        "lcobucci/jwt": "^4.0@dev",
        "phpmyadmin/sql-parser": "^5.1",
        "laravel/framework": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Bima\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Bima\\Client\\BimaClientServiceProvider"
            ]
        }
    }
}
